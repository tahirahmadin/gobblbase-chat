{
  "rewrites": [
    {
      "source": "/:username",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*(bot|facebook|twitter|linkedin|discord|whatsapp|telegram).*"
        }
      ],
      "destination": "https://kifortestapi.gobbl.ai/meta/:username"
    }
  ]
}
