{
  "rewrites": [
    {
      "source": "/:username",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "bot"
        }
      ],
      "destination": "https://kifortestapi.gobbl.ai/meta/:username"
    },
    {
      "source": "/:username",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "facebook"
        }
      ],
      "destination": "https://kifortestapi.gobbl.ai/meta/:username"
    },
    {
      "source": "/:username",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "twitter"
        }
      ],
      "destination": "https://kifortestapi.gobbl.ai/meta/:username"
    }
  ]
}
